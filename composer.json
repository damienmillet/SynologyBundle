{
    "name": "damienmillet/synology-bundle",
    "description": "Synology Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "damien millet",
            "email": "contact@damien-millet.xyz"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DM\\SynologyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DM\\SynologyBundle\\Test\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "symfony/config": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/http-client": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/orm-pack": "^2.0",
        "symfony/security-bundle": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
    },
    "scripts": {
        "pre-package-install":[
            "DM\\SynologyBundle::postPackageInstall"
        ],
        "pre-update-install":[
            "DM\\SynologyBundle::postPackageInstall"
        ],
        "post-package-uninstall": [
            "DM\\SynologyBundle::postPackageUninstall"
        ]
    }
}
